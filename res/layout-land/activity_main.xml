<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="637dp"
        android:layout_height="526dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <EditText
            android:id="@+id/historialLabel"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/txtMemoriaEditar"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="527dp"
            android:layout_marginBottom="-1dp"
            android:ems="10"
            android:focusable="false"
            android:inputType="textPersonName"
            android:text="Memoria:"
            android:textSize="24sp" />

        <EditText
            android:id="@+id/txtMemoriaEditar"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="278dp"
            android:layout_marginEnd="527dp"
            android:layout_marginRight="527dp"
            android:layout_marginBottom="195dp"
            android:clickable="true"
            android:ems="10"
            android:focusable="false"
            android:inputType="textPersonName"
            android:textAlignment="textEnd"
            android:textColor="#00FFFFFF"
            android:textSize="24sp"
            android:visibility="visible" />

        <EditText
            android:id="@+id/txtNumeroEditar"
            android:layout_width="601dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="17dp"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="217dp"
            android:layout_marginEnd="19dp"
            android:layout_marginRight="19dp"
            android:ems="10"
            android:focusable="false"
            android:inputType="textPersonName"
            android:textAlignment="textEnd"
            android:textSize="24sp" />

        <Button
            android:id="@+id/botonResta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/botonSuma"
            android:layout_alignStart="@+id/botonSuma"
            android:layout_alignLeft="@+id/botonSuma"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="-"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtNumeroEditar"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="135dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="10dp"
            android:text="1"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/boton_1"
            android:layout_alignBottom="@+id/boton_1"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/boton_1"
            android:layout_toRightOf="@+id/boton_1"
            android:text="2"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/boton_2"
            android:layout_alignBottom="@+id/boton_2"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/boton_2"
            android:layout_toRightOf="@+id/boton_2"
            android:text="3"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/boton_5"
            android:layout_alignBottom="@+id/boton_5"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/boton_5"
            android:layout_toRightOf="@+id/boton_5"
            android:text="6"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_2"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@+id/boton_4"
            android:layout_toRightOf="@+id/boton_4"
            android:text="5"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_1"
            android:layout_alignStart="@+id/boton_1"
            android:layout_alignLeft="@+id/boton_1"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:text="4"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_4"
            android:layout_alignStart="@+id/boton_4"
            android:layout_alignLeft="@+id/boton_4"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:text="7"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_5"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@+id/boton_7"
            android:layout_toRightOf="@+id/boton_7"
            android:text="8"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_6"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@+id/boton_5"
            android:layout_toRightOf="@+id/boton_5"
            android:text="9"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonMulti"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_6"
            android:layout_alignStart="@+id/botonResta"
            android:layout_alignLeft="@+id/botonResta"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="*"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonDiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_9"
            android:layout_alignStart="@+id/botonMulti"
            android:layout_alignLeft="@+id/botonMulti"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="/"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonPow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_9"
            android:layout_alignStart="@+id/botonMulti"
            android:layout_alignLeft="@+id/botonMulti"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="48dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="Pow"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonSqrt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_9"
            android:layout_alignStart="@+id/botonMulti"
            android:layout_alignLeft="@+id/botonMulti"
            android:layout_marginStart="-88dp"
            android:layout_marginLeft="-88dp"
            android:layout_marginTop="48dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="sqrt"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/boton_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_8"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@+id/boton_7"
            android:layout_toRightOf="@+id/boton_7"
            android:text="0"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonDecimal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boton_7"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:layout_toStartOf="@+id/boton_8"
            android:layout_toLeftOf="@+id/boton_8"
            android:text="."
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonRespuesta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/boton_3"
            android:layout_marginStart="-88dp"
            android:layout_marginLeft="-88dp"
            android:layout_marginTop="192dp"
            android:layout_toEndOf="@+id/boton_0"
            android:layout_toRightOf="@+id/boton_0"
            android:backgroundTint="@android:color/holo_green_light"
            android:text="="
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonLimpiar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/botonDiv"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="414dp"
            android:layout_marginRight="414dp"
            android:backgroundTint="@android:color/holo_blue_dark"
            android:text="clear"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonDelete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/botonDiv"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="-49dp"
            android:layout_marginEnd="238dp"
            android:layout_marginRight="238dp"
            android:backgroundTint="@android:color/holo_blue_dark"
            android:text="Delete"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/botonSuma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/boton_6"
            android:layout_alignParentEnd="false"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="150dp"
            android:layout_marginRight="150dp"
            android:layout_marginBottom="0dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="+"
            android:textColor="@android:color/background_light"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="97dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/txtNumeroEditar"
            android:layout_marginTop="253dp"
            android:layout_marginEnd="38dp"
            android:layout_toStartOf="@+id/botonDecimal"
            android:onClick="nuevaVentana"
            android:text="Autores"
            android:visibility="visible" />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>